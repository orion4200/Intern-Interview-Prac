class Solution
{
    public:
    //Function to reverse a linked list.
    struct Node* reverseList(struct Node *head)
    {
        Node *p, *q, *r ;
        p = head ;
        q = r = NULL ;
        
        while(p != NULL)
        {
            r = q ;
            q = p ;
            p = p->next ;
            q->next = r ;
        }
        
        head = q ;
        return head ;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       ;
    }
    
};
  
